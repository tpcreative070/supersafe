<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorButton</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:textColorPrimary">@color/colorButton</item>
        <item name="android:textColorSecondary">@color/colorButton</item>
        <item name="android:windowDisablePreview">true</item>


        <item name="preference_icon">@color/material_gray_600</item>
        <item name="preference_title">@color/material_gray_800</item>
        <item name="preference_summary">@color/material_gray_600</item>

        <item name="android:divider">@color/colorButton</item>
        <!--Change menu item for text color -->
        <item name="android:itemTextAppearance">@style/menu_item_color</item>
        /*Preference theme*/
        <item name="preferenceTheme">@style/PreferenceThemeOverlay.v14.Material</item>
        <!--Action mode -->
        <item name="actionModeStyle">@style/CustomActionModeStyle</item>
        <item name="windowActionModeOverlay">true</item>


        <!--Animation transfer activity-->
       <item name="android:windowAnimationStyle">@style/WindowAnimationTransition</item>

